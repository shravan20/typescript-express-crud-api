{
    "extends": ["tslint:recommended", "tslint-clean-code"],
    "rules": {
        "member-ordering": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "no-commented-out-code": true,
        "quotemark": [
        true,
        "single",
        "jsx-double",
        "avoid-escape"
        ],
        "trailing-comma": [
        true,
        {
            "multiline": "always",
            "singleline": "never"
        }
        ]
    },
    "linterOptions": {
        "exclude": [
            "./node_modules/**"
        ]
    },
    "jsRules": true
}